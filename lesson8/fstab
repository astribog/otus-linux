/dev/mapper/otus-root /                       xfs     defaults        0 0
UUID=c6177e87-c955-4932-b6bf-c34feca98da4 /boot                   xfs     defaults        0 0
/dev/mapper/otus-swap swap                    swap    defaults        0 0
